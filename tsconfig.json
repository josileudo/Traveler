{
  "compilerOptions": {
    "lib": ["DOM", "com.interable", "ESNext"],
    "allowJs": true,
    "jsx": "react-jsx",
    "noEmit": true,
    "strict": true,
    "moduleResolution": "node",
    "resolveJsonModules": true,
    "isolatedModules": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop":true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}